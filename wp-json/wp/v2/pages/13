l-content').not('#' + showme).animate({ opacity: 0 }, 300, function () {\r\n                $(this).slideUp(300); \/\/ Slide up after opacity reaches 0\r\n            });\r\n\r\n            \/\/ Smooth fade-in for the selected section\r\n            $('#' + showme)\r\n                .stop(true, true)\r\n                .slideDown(300, function () {\r\n                    $(this).css('display', 'block').animate({ opacity: 1 }, 300);\r\n                });\r\n        });\r\n    });\r\n<\/script>\t\t\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t\t\t<div class=\"elementor-element elementor-element-b629f89 elementor-widget elementor-widget-html\" data-id=\"b629f89\" data-element_type=\"widget\" data-widget_type=\"html.default\">\n\t\t\t\t<div class=\"elementor-widget-container\">\n\t\t\t\t\t<!DOCTYPE html>\r\n<html lang=\"en\">\r\n<head>\r\n    <meta charset=\"UTF-8\">\r\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\r\n    <title>Preloader with Video<\/title>\r\n    <style>\r\n        @import url('https:\/\/fonts.googleapis.com\/css2?family=Chakra+Petch:wght@300;400;700&display=swap');\r\n\r\n        * {\r\n            margin: 0;\r\n            padding: 0;\r\n            box-sizing: border-box;\r\n        }\r\n        html, body {\r\n            overflow: hidden; \/* Prevent scrolling by default *\/\r\n            width: 100%;\r\n            height: 100%;\r\n        }\r\n        #preloader {\r\n            position: fixed;\r\n            top: 0;\r\n            left: 0;\r\n            width: 100%;\r\n            height: 100%;\r\n            background: black;\r\n            display: flex;\r\n            justify-content: center;\r\n            align-items: center;\r\n            z-index: 999999;\r\n        }\r\n        #intro-video {\r\n            width: 100%;\r\n            height: 100%;\r\n            object-fit: cover;\r\n            display: block;\r\n        }\r\n        #logo-container {\r\n            display: none;\r\n            justify-con